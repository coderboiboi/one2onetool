pipeline {
    agent any
    stages {
    	stage('git-checkout') {
            steps {
                echo 'Checkout from git repo'
                git 'https://github.com/coderboiboi/one2onetool.git'
                }
            }
        stage('build') {
            steps {
                echo 'build project'
                sh '''deployment/build.sh'''
                }
            }
        stage('test') {
            steps {
                echo 'test project'
                sh '''deployment/test.sh'''
                }
            }
        stage('deploy') {
            steps {
                echo 'deploy project'
                sh '''deployment/deploy.sh'''
                }
            }

    }
}

